                                                            <Separator Height="8" Background="Transparent"/>
                                                            <Grid>
                                                                <Rectangle Height="25" Width="200" HorizontalAlignment="Left" Fill="White" RadiusX="5" RadiusY="5" Stroke="#FFE1E1E1"/>
                                                                <TextBox x:Name="txtStudentEmail" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Inter" FontSize="14" Foreground="#3F3F3F" Background="Transparent" BorderThickness="0" Width="200" TextChanged="txtAuthorSearch_TextChanged"/>
                                                            </Grid>
                                                            <Separator Height="8" Background="Transparent"/>
                                                            <Grid>
                                                                <Rectangle Height="25" Width="200" HorizontalAlignment="Left" Fill="White" RadiusX="5" RadiusY="5" Stroke="#FFE1E1E1"/>
                                                                <TextBox x:Name="txtStudentPassword" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Inter" FontSize="14" Foreground="#3F3F3F" Background="Transparent" BorderThickness="0" Width="200" TextChanged="txtAuthorSearch_TextChanged"/>
                                                            </Grid>
                                                            <Separator Height="9" Background="Transparent"/>
                                                        </StackPanel>
                                                    </Grid>
                                                    <Separator Height="20" Background="Transparent"/>
                                                    <Grid>
                                                        <Grid.RowDefinitions>
                                                            <RowDefinition Height="*"/>
                                                            <RowDefinition Height="*"/>
                                                            <RowDefinition Height="*"/>
                                                        </Grid.RowDefinitions>
                                                        <Grid x:Name="studAddButton" Grid.Row="0" Margin="15 15 15 0" Height="40" MouseLeftButtonDown="studAddButton_MouseLeftButtonDown" Cursor="Hand">
                                                            <Border CornerRadius="8" Background="#485BFF"/>
                                                            <TextBlock Text="Add" VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Inter" FontSize="14" FontWeight="SemiBold" Foreground="White"/>
                                                        </Grid>
                                                        <Grid x:Name="studUpdateButton" Grid.Row="1" Margin="15 15 15 0" Height="40" MouseLeftButtonDown="studUpdateButton_MouseLeftButtonDown" Cursor="Hand">
                                                            <Border CornerRadius="8" Background="#485BFF"/>
                                                            <TextBlock Text="Update" VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Inter" FontSize="14" FontWeight="SemiBold" Foreground="White"/>
                                                        </Grid>
                                                        <Grid x:Name="studDeleteButton" Grid.Row="2" Margin="15 15 15 0" Height="40" MouseLeftButtonDown="studDeleteButton_MouseLeftButtonDown" Cursor="Hand">
                                                            <Border CornerRadius="8" Background="#485BFF"/>
                                                            <TextBlock Text="Delete" VerticalAlignment="Center" HorizontalAlignment="Center" FontFamily="Inter" FontSize="14" FontWeight="SemiBold" Foreground="White"/>
                                                        </Grid>

                                                    </Grid>
                                                </StackPanel>
                                            </Grid>
                                        </StackPanel>
                                    </Grid>
                                </Grid>
                            </Grid>
                            <Separator Height="15" Background="Transparent"/>

                        </StackPanel>

                    </Grid>
                    <Grid x:Name="histView" Visibility="Hidden">
                        <StackPanel>
                            <Separator Height="16" Background="Transparent"/>
                            <TextBlock Grid.Column="0" Text="History" FontFamily="Inter" Margin="20 3 0 0" FontSize="23" Foreground="#3F3F3F"/>
                            <TextBlock Text="All recent returned books are here!" FontFamily="Inter" Margin="20 0 0 0" FontSize="13" Foreground="#3F3F3F"/>
                            <Grid  Margin="0 0 0 20">
                                <StackPanel>
                                    <Separator Height="42" Background="Transparent"/>
                                    <Grid Margin="20 0 20 0" Background="Transparent">
                                        <Rectangle Height="25" Width="290" HorizontalAlignment="Left" Fill="White" RadiusX="5" RadiusY="5" Stroke="#FFE1E1E1"/>
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="30"/>
                                                <ColumnDefinition Width="*"/>
                                            </Grid.ColumnDefinitions>
                                            <Viewbox Grid.Column="0" Height="15">
                                                <Path Data="M10 18C5.58172 18 2 14.4183 2 10C2 5.58172 5.58172 2 10 2C14.4183 2 18 5.58172 18 10C18 11.8487 17.3729 13.551 16.3199 14.9056L21.7071 20.2929L20.2929 21.7071L14.9056 16.3199C13.551 17.3729 11.8487 18 10 18ZM16 10C16 13.3137 13.3137 16 10 16C6.68629 16 4 13.3137 4 10C4 6.68629 6.68629 4 10 4C13.3137 4 16 6.68629 16 10Z" Fill="#7D7D7D"/>
                                            </Viewbox>
                                            <TextBox x:Name="txtHistSearch" Grid.Column="1" HorizontalAlignment="Left" VerticalAlignment="Center" FontFamily="Inter" FontSize="14" Foreground="#3F3F3F" Background="Transparent" BorderThickness="0" Width="250" TextChanged="txtHistSearch_TextChanged"/>
                                        </Grid>
                                    </Grid>
                                    <Separator Height="15" Background="Transparent"/>
                                    <Grid Margin="20 0 20 0" >
                                        <Rectangle Height="438" Fill="White" RadiusX="10" RadiusY="10">
                                            <Rectangle.Effect>
                                                <DropShadowEffect Color="DimGray" Direction="270" ShadowDepth="10" Opacity="0.1" BlurRadius="50"/>
                                            </Rectangle.Effect>
                                        </Rectangle>
                                        <DataGrid Style="{DynamicResource DataGridStyle1}" CellStyle="{DynamicResource DataGridCellStyle1}" ColumnHeaderStyle="{DynamicResource DataGridColumnHeaderStyle1}" RowStyle="{DynamicResource DataGridRowStyle1}" x:Name="histDataGrid" Height="446" Margin="20 0 20 0">
                                            <DataGrid.Columns>
                                                <DataGridTextColumn Header="Borrow ID" Binding="{Binding Return_Borrow_ID}" CanUserResize="False" Width="*"/>
                                                <DataGridTemplateColumn Header="Book ID" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_Borrow_Book_ID}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="Student ID" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_Student_ID}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="Name" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_Name}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="Title" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_Title}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="ISBN" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_ISBN}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="Author" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_Author}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="Borrow Date" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_Borrow_Date}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>
                                                <DataGridTemplateColumn Header="Return Date" IsReadOnly="True" Width="*">
                                                    <DataGridTemplateColumn.CellTemplate>
                                                        <DataTemplate>
                                                            <StackPanel Orientation="Horizontal">
                                                                <TextBlock Text="{Binding Return_Date}" Margin="0 0 10 0"/>
                                                            </StackPanel>
                                                        </DataTemplate>
                                                    </DataGridTemplateColumn.CellTemplate>
                                                </DataGridTemplateColumn>

                                            </DataGrid.Columns>
                                        </DataGrid>
                                    </Grid>
                                </StackPanel>
                            </Grid>
                        </StackPanel>
                    </Grid>

                </Grid>
            </Grid>
        </Grid>
        <Grid x:Name="informationGrid" Width="300" Height="200" VerticalAlignment="Center" HorizontalAlignment="Center" Visibility="Hidden">
            <Border Background="White" CornerRadius="20" BorderThickness="1" BorderBrush="#7D7D7D"/>
            <StackPanel>
                <Grid Height="40">
                    <Border Background="#485BFF" CornerRadius="20 20 0 0" BorderThickness="1" BorderBrush="#7D7D7D">
                        <TextBlock Text="Information" FontFamily="Inter" FontWeight="Bold" FontSize="15" Foreground="White" Margin="20 10"/>
                    </Border>
                </Grid>
                <Separator Height="10" Background="Transparent"/>
                <Grid Height="145">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="3*"/>
                        <RowDefinition Height="*"/>
                    </Grid.RowDefinitions>
                    <TextBlock x:Name="txtInformation" Text="lorem ipsum dolor" Margin="20 0 20 0" FontFamily="Inter" FontSize="15" Foreground="#7D7D7D" TextWrapping="Wrap"/>
                    <Grid x:Name="okButton" Grid.Row="1" MouseLeftButtonDown="okButton_MouseLeftButtonDown" Cursor="Hand" Visibility="Hidden">
                        <Border Background="White" CornerRadius="0 0 20 20" BorderThickness="1" BorderBrush="#7D7D7D"/>
                        <TextBlock Text="Ok" FontFamily="Inter" FontSize="15" Foreground="#7D7D7D" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                    </Grid>
                    <Grid x:Name="canyesButton" Grid.Row="1">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>
                        <Grid x:Name="cancelButton"  Grid.Column="0" Cursor="Hand" MouseLeftButtonDown="cancelButton_MouseLeftButtonDown">
                            <Border Background="White" CornerRadius="0 0 0 20" BorderThickness="1" BorderBrush="#7D7D7D"/>
                            <TextBlock Text="Cancel" FontFamily="Inter" FontSize="15" Foreground="#7D7D7D" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                        </Grid>
                        <Grid x:Name="yesButton" Grid.Column="2" Cursor="Hand" MouseLeftButtonDown="yesButton_MouseLeftButtonDown">
                            <Border Background="White" CornerRadius="0 0 20 0" BorderThickness="1" BorderBrush="#7D7D7D"/>
                            <TextBlock Text="Yes" FontFamily="Inter" FontSize="15" Foreground="#7D7D7D" VerticalAlignment="Center" HorizontalAlignment="Center"/>
                        </Grid>
                    </Grid>
                </Grid>
            </StackPanel>
        </Grid>
    </Grid>
</Window>
